<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="musicPageVM" type="com.andreyrodriguezsapplication.app.modules.musicpage.data.viewmodel.MusicPageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameStackgrouptwo"
        android:layout_height="match_parent" android:layout_width="match_parent">
        <FrameLayout android:id="@+id/frameMusicpage"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylegray_900">
            <LinearLayout android:id="@+id/linearColumngrouptwo"
                android:layout_gravity="center"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_27pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <androidx.appcompat.widget.Toolbar
                    android:gravity="center_horizontal"
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearRowgrouptwo"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageButton
                            android:background="@drawable/rectangle_bg_white_a700_0a_radius_16"
                            android:id="@+id/btnGroupTwo"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_width="@dimen/_32pxh"
                            android:paddingHorizontal="@dimen/_10pxh"
                            android:paddingVertical="@dimen/_10pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_group15" tools:ignore="ContentDescription"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtNowplaying"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginStart="@dimen/_80pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_weight="0.75"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{musicPageVM.musicPageModel.txtNowplaying}"
                            style="@style/txtSatoshibold18" tools:text="@string/lbl_now_playing"/>
                        <LinearLayout
                            android:gravity="center_horizontal"
                            android:id="@+id/linearColumnellipseeight"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginStart="@dimen/_94pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_weight="0.03"
                            android:layout_width="0dp" android:orientation="vertical">
                            <View
                                android:background="@drawable/rectangle_bg_gray_305_radius_1_965"
                                android:id="@+id/viewEllipseEight"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_3pxh" android:layout_width="@dimen/_3pxh"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_305_radius_1_965"
                                android:id="@+id/viewEllipseOne"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_305_radius_1_965"
                                android:id="@+id/viewEllipseFive"
                                android:layout_gravity="center_horizontal"
                                android:layout_height="@dimen/_3pxh"
                                android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
                        </LinearLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <FrameLayout android:id="@+id/frameStackellipsetwo"
                    android:layout_height="@dimen/_370pxv"
                    android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_335pxh">
                    <View
                        android:background="@drawable/rectangle_radius_15"
                        android:id="@+id/viewEllipseTwo"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_30pxh"
                        android:layout_marginBottom="@dimen/_132pxv"
                        android:layout_marginEnd="@dimen/_115pxh"
                        android:layout_marginStart="@dimen/_115pxh"
                        android:layout_marginTop="@dimen/_132pxv" android:layout_width="@dimen/_30pxh"/>
                    <ImageView android:id="@+id/imageRectangleSeven"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_370pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        cornerRadius="@{@dimen/_30pxh}"
                        imageRes="@{@drawable/img_rectangle7}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle7"/>
                </FrameLayout>
                <LinearLayout android:id="@+id/linearRowbadguy"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnbadguy"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.94"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBadGuy"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{musicPageVM.musicPageModel.txtBadGuy}"
                            style="@style/txtSatoshibold20" tools:text="@string/lbl_bad_guy"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBillieEilish"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{musicPageVM.musicPageModel.txtBillieEilish}"
                            style="@style/txtSatoshiregular20" tools:text="@string/lbl_billie_eilish"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageClock"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_19pxv"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacklineone"
                    android:layout_height="@dimen/_16pxv"
                    android:layout_marginTop="@dimen/_45pxv" android:layout_width="@dimen/_335pxh">
                    <View android:background="@color/bluegray_400_4c"
                        android:id="@+id/lineLineOne"
                        android:layout_gravity="bottom|center"
                        android:layout_height="@dimen/_2pxv"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_4pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_331pxh"/>
                    <View android:background="@color/gray_402"
                        android:id="@+id/lineLineTwo"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_162pxh"/>
                    <View
                        android:background="@drawable/rectangle_bg_gray_402_radius_8"
                        android:id="@+id/viewEllipseSix"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginEnd="@dimen/_148pxh"
                        android:layout_marginStart="@dimen/_148pxh" android:layout_width="@dimen/_16pxh"/>
                </FrameLayout>
                <LinearLayout
                    android:id="@+id/linearRowtwohundredtwentyfive"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTwoHundredTwentyFive"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.47"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{musicPageVM.musicPageModel.txtTwoHundredTwentyFive}"
                        style="@style/txtSatoshibold12" tools:text="@string/lbl_2_25"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTime"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_280pxh"
                        android:layout_weight="0.53"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{musicPageVM.musicPageModel.txtTime}"
                        style="@style/txtSatoshibold12" tools:text="@string/lbl_4_02"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRow"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_74pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowrefresh"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageRefresh"
                            android:layout_height="@dimen/_19pxh"
                            android:layout_width="@dimen/_17pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_refresh"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                        <ImageView android:id="@+id/imageForward"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_36pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_forward"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowforwardOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_116pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.5"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageForwardOne"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_15pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_forward_14x15"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_forward_14x15"/>
                        <ImageView android:id="@+id/imageCall"
                            android:layout_height="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_37pxh"
                            android:layout_width="@dimen/_16pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_call"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                    </LinearLayout>
                </LinearLayout>
                <ImageView android:id="@+id/imageArrowup"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_7pxh"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_69pxv"
                    android:layout_width="@dimen/_14pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowup"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLyrics"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{musicPageVM.musicPageModel.txtLyrics}"
                    style="@style/txtSatoshibold14_2" tools:text="@string/lbl_lyrics"/>
            </LinearLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/green_500"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_110pxv"
            android:layout_marginEnd="@dimen/_158pxh"
            android:layout_marginStart="@dimen/_158pxh"
            android:layout_marginTop="@dimen/_110pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_72pxh"
            app:srcCompat="@drawable/img_map" app:tint="@color/white_A700"/>
    </FrameLayout>
</layout>
