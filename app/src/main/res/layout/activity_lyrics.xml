                                    android:id="@+id/txtBadGuyOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginStart="@dimen/_1pxh"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{lyricsVM.lyricsModel.txtBadGuyOne}"
                                    style="@style/txtSatoshibold16_4" tools:text="@string/lbl_bad_guy"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtBillieEilish"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_9pxv"
                                    android:layout_width="wrap_content"
                                    android:singleLine="true"
                                    android:text="@{lyricsVM.lyricsModel.txtBillieEilish}"
                                    style="@style/txtSatoshiregular12_5" tools:text="@string/lbl_billie_eilish"/>
