                            android:background="@drawable/rectangle_bg_gray_607_radius_1_965"
                            android:id="@+id/viewEllipseOne"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_3pxh"
                            android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
                        <View
                            android:background="@drawable/rectangle_bg_gray_607_radius_1_965"
                            android:id="@+id/viewEllipseFive"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_3pxh"
                            android:layout_marginTop="@dimen/_3pxv" android:layout_width="@dimen/_3pxh"/>
