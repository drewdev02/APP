                                android:src="@drawable/img_clock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                        </LinearLayout>
                        <FrameLayout android:id="@+id/frameStacklineone"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_16pxv"
                            android:layout_marginEnd="@dimen/_31pxh"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_9pxv" android:layout_width="@dimen/_316pxh">
                            <View
                                android:background="@color/black_900_4c"
                                android:id="@+id/lineLineOne"
                                android:layout_gravity="bottom|center"
                                android:layout_height="@dimen/_2pxv"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginStart="@dimen/_5pxh"
                                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_311pxh"/>
                            <View android:background="@color/gray_402"
                                android:id="@+id/lineLineTwo"
                                android:layout_gravity="bottom|start"
                                android:layout_height="@dimen/_2pxv"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_162pxh"/>
                            <View
                                android:background="@drawable/rectangle_bg_gray_402_radius_8"
                                android:id="@+id/viewEllipseSix"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_16pxh"
                                android:layout_marginEnd="@dimen/_40pxh"
                                android:layout_marginStart="@dimen/_40pxh" android:layout_width="@dimen/_16pxh"/>
                        </FrameLayout>
                        <LinearLayout
                            android:id="@+id/linearRowtwohundredtwentyfive"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_31pxh"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTwoHundredTwentyFive"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_weight="0.47"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{lyricsVM.lyricsModel.txtTwoHundredTwentyFive}"
                                style="@style/txtSatoshibold12" tools:text="@string/lbl_2_25"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTime"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:layout_marginStart="@dimen/_266pxh"
                                android:layout_weight="0.53"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{lyricsVM.lyricsModel.txtTime}"
                                style="@style/txtSatoshibold12" tools:text="@string/lbl_4_02"/>
                        </LinearLayout>
                        <LinearLayout android:id="@+id/linearRow"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_39pxv"
                            android:layout_marginEnd="@dimen/_31pxh"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_marginTop="@dimen/_25pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <LinearLayout
                                android:id="@+id/linearRowrefresh"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.5"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageRefresh"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_refresh"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_refresh"/>
                                <ImageView
                                    android:id="@+id/imageForwardOne"
                                    android:layout_height="@dimen/_11pxh"
                                    android:layout_marginBottom="@dimen/_2pxv"
                                    android:layout_marginStart="@dimen/_37pxh"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_forward"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_forward"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearRowforwardTwo"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_114pxh"
                                android:layout_weight="0.5"
                                android:layout_width="0dp" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageForwardTwo"
                                    android:layout_height="@dimen/_11pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_12pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_forward_14x15"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_forward_14x15"/>
                                <ImageView android:id="@+id/imageCall"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_marginStart="@dimen/_38pxh"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_call"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_call"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:backgroundTint="@color/green_500"
            android:id="@+id/floatingBtnFloatingactionbutton"
            android:layout_gravity="bottom|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_169pxh"
            android:layout_marginStart="@dimen/_169pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="wrap_content"
            app:fabCustomSize="@dimen/_51pxh"
            app:srcCompat="@drawable/img_map" app:tint="@color/white_A700"/>
    </FrameLayout>
</layout>
