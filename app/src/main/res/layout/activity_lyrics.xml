                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_gravity="start"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_26pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{lyricsVM.lyricsModel.txtLanguage}"
                            style="@style/txtSatoshibold15" tools:text="@string/lbl_verse_1"/>
                        <LinearLayout android:id="@+id/linearRowforward"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_9pxh"
                            android:layout_marginTop="@dimen/_28pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageForward"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginBottom="@dimen/_26pxv"
                                android:layout_marginTop="@dimen/_264pxv"
                                android:layout_width="@dimen/_10pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_forward_11x10"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_forward_11x10"/>
                            <FrameLayout
                                android:id="@+id/frameStackellipsetwo"
                                android:layout_height="@dimen/_301pxv"
                                android:layout_marginStart="@dimen/_11pxh" android:layout_width="@dimen/_270pxh">
                                <View
                                    android:background="@drawable/rectangle_radius_15"
                                    android:id="@+id/viewEllipseTwo"
                                    android:layout_gravity="bottom|start"
                                    android:layout_height="@dimen/_30pxh"
                                    android:layout_marginBottom="@dimen/_61pxv"
                                    android:layout_marginEnd="@dimen/_87pxh"
                                    android:layout_marginStart="@dimen/_87pxh"
                                    android:layout_marginTop="@dimen/_61pxv" android:layout_width="@dimen/_30pxh"/>
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtDescription"
                                    android:layout_gravity="start"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:text="@{lyricsVM.lyricsModel.txtDescription}"
                                    style="@style/txtSatoshibold16_3" tools:text="@string/msg_sleepin_you_r"/>
