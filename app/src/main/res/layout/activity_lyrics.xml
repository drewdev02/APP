                            android:layout_marginStart="@dimen/_14pxh"
                            android:layout_marginTop="@dimen/_13pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{lyricsVM.lyricsModel.txtLanguageOne}"
                            style="@style/txtSatoshibold15" tools:text="@string/lbl_verse_2"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearColumnellipseeight"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_410pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.01"
                        android:layout_width="0dp" android:orientation="vertical">
                        <View
                            android:background="@drawable/rectangle_bg_gray_607_radius_1_965"
                            android:id="@+id/viewEllipseEight"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_3pxh" android:layout_width="@dimen/_3pxh"/>
