                cornerRadius="@{@dimen/_47pxh}"
                imageRes="@{@drawable/img_rectangle7_844x390}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle7_844x390"/>
            <LinearLayout android:id="@+id/linearColumndescriptionOne"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowlanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout
                        android:id="@+id/linearColumngrouptwentyseven"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.99"
                        android:layout_width="0dp" android:orientation="vertical">
                        <androidx.appcompat.widget.Toolbar
                            android:gravity="start"
                            android:id="@+id/toolbarToolbar"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent" app:contentInsetStart="0dp">
                            <LinearLayout
                                android:id="@+id/linearRowgrouptwentyseven"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <ImageButton
                                    android:background="@drawable/rectangle_bg_white_a700_0a_radius_16"
                                    android:id="@+id/btnGroupTwentySeven"
                                    android:layout_height="@dimen/_32pxh"
                                    android:layout_width="@dimen/_32pxh"
                                    android:paddingHorizontal="@dimen/_10pxh"
                                    android:paddingVertical="@dimen/_10pxv"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/img_group15" tools:ignore="ContentDescription"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtBadGuy"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginStart="@dimen/_100pxh"
                                    android:layout_marginTop="@dimen/_7pxv"
                                    android:layout_weight="0.71"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{lyricsVM.lyricsModel.txtBadGuy}"
                                    style="@style/txtSatoshibold20_3" tools:text="@string/lbl_bad_guy"/>
                            </LinearLayout>
